{
  "name": "airygen/laravel-amqp-producer",
  "description": "RabbitMQ publisher for Laravel with confirms, retries, DLX strategy.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.2",
    "illuminate/support": "^11.0",
    "php-amqplib/php-amqplib": "^3.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.3",
    "orchestra/testbench": "^9.0",
    "laravel/pint": "^1.17",
    "phpstan/phpstan": "^1.11",
    "squizlabs/php_codesniffer": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Airygen\\RabbitMQ\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Airygen\\RabbitMQ\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Airygen\\RabbitMQ\\RabbitMQServiceProvider"
      ]
    }
  },
  "scripts": {}
}
